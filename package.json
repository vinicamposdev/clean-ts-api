    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.9",
    "rimraf": "^3.0.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.5",
