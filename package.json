    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "sucrase": "^3.19.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
